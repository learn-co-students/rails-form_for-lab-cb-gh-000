<h1>School Class Form</h1>

<%= form_for :school_class, url: school_classes_path do |f| %>
  <div>
    <label>Title</label>
    <%= f.text_field :title %>
  </div>
  <div>
    <label>Room Number</label>
    <%= f.number_field :room_number %>
  </div>
  <button>Create School Class</button>
<% end %>
